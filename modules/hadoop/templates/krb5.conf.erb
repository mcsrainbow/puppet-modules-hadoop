[logging]
 default = FILE:/var/log/krb5libs.log
 kdc = FILE:/var/log/krb5kdc.log
 admin_server = FILE:/var/log/kadmind.log

[libdefaults]
 default_realm = US-WEST-1.COMPUTE.INTERNAL
 dns_lookup_realm = false
 dns_lookup_kdc = false
 ticket_lifetime = 24h
 renew_lifetime = 7d
 forwardable = true
 udp_preference_limit =1 

[realms]
 US-WEST-1.COMPUTE.INTERNAL = {
  kdc = ip-10-197-29-251.us-west-1.compute.internal
  admin_server = ip-10-197-29-251.us-west-1.compute.internal
  default_domain = us-west-1.compute.internal
 }

[domain_realm]
 .us-west-1.compute.internal = US-WEST-1.COMPUTE.INTERNAL
 us-west-1.compute.internal = US-WEST-1.COMPUTE.INTERNAL
